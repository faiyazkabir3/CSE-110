public class test {
public static void main(String[]args){
Scanner sc= new Scanner(System.in);
System.out.print("Enter your number = ");
int n=sc.nextInt();

int i= 0;

while (n!=0) {
n= n/10;
i++;
}
System.out.print("Total digits = "+i);
//System.out.println("Total digits = "+n);


}
}
//10
import java.util.Scanner;

public class Test {
   public static void main(String[] args) {
Scanner sc = new Scanner(System.in);
     System.out.print("Enter your number = ");//prompt
  int n = sc.nextInt();//input
int temp = n;//temp file nedded to save n's value 
int i = 0;
while (n != 0) {
n = n / 10;//same as digit counter
i++;
}
        i = i - 1;

     //System.out.print("Total digits = "+i);
 while (temp > 0) { // 1234
   int v2 = (int) Math.pow(10, i);//10*3// 1
    int digit = temp / v2;//10*3// 1
          System.out.print(digit+ " , " );
            temp = temp % v2;//234
            i--;
        }
        
    }
}




//11(a)
import java.util.Scanner;
public class test {
public static void main(String[]args){
 Scanner sc= new Scanner(System.in);
System.out.print("Enter your number= ");//prompt dilam
 int a=sc.nextInt();

 int i= 1;
 int dicounter=0;
 if(a<0){
  System.out.printf(a+" is not a prime number"); 
  System.exit(0);//jumper seat yeyy
 }
while (i<=a) {
if (a%i==0){
dicounter++;//counter updating
}
i++;//  post increment 
}
if(dicounter==2){// prime numbers are only divisable by 2 numbers
  System.out.printf(a+" is a prime number");  
}
else{
  System.out.printf(a+" is not a prime number");     
}
}
}


//11(b)
import java.util.Scanner;
public class test {
public static void main(String[]args){
 Scanner sc= new Scanner(System.in);
System.out.print("Enter your number= ");//prompt dilam
 int a24=sc.nextInt();
 if (a24<=0){
 System.out.print(a24+" is not a perfect number");   
 System.exit(0);//oreh baba arekta jumper seat
 }
int sum=0;
 int i= 1;
while (i<a24) {
if (a24%i==0){
sum=sum+i;
}
i++;//  post increment 
}
if (sum==a24){//A number is said to be a perfect number if the sum of its divisors, including 1 but not the number itself is equal to that number
    System.out.println (a24+" is a perfect number");
}
else {
       System.out.println (a24+" is not a perfect number");
 
}
}
}


