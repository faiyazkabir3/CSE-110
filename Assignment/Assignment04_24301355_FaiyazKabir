//2
import java.util.Scanner;
public class test {
 public static void main(String[]args){
 Scanner sc= new Scanner(System.in);
 System.out.print("how many number= ");//i took input
 int a=sc.nextInt();//scanning
int i= 1;
 int sum=0;
 while (i<=a) {//wow loop very cool 
int a1=sc.nextInt();
 sum= sum+a1;
 i++;
 }
 System.out.printf("\n The sum of %d no is: %d ",a,sum);// here comes the output
 int avg = sum/a;
System.out.printf(" \n The Average is %d ",avg);//// here comes the output2

 }

}

//3
import java.util.Scanner;
public class test {
 public static void main(String[]args){
 Scanner sc= new Scanner(System.in);

int i= 1;

 while (i>0) {
System.out.print("\nEnter your number= ");//prompt
int a1=sc.nextInt();//input
if (a1>=0){
int sq = a1*a1;//formula
System.out.printf("\n%d^%d=%d ",a1,a1,sq);//square 
}
else if(a1<0) {
    
    System.out.print("end");
    i--;
}

}
 }

}

