import java.util.Scanner;
public class test {
public static void main(String[]args){
 Scanner sc= new Scanner(System.in);
System.out.print("Enter your number= ");//prompt dilam
 int a=sc.nextInt();

 int i= 1;

System.out.printf("\n Divisors of %d\n",a);
while (i<=a) {
if (a%i==0){
System.out.println (i);//hmmm... ashol output

}
i++;//  post increment 
}
}

}



//5
import java.util.Scanner ;
public class test {
    public static void main(String[]args){
        Scanner sc = new Scanner(System.in);
        System.out.print("Enter the numbers of  integer: ");//hmmm input nisi
        int n= sc.nextInt();
        int poscounter = 0;
        int negcounter =0;
       // System.out.print(a);
       int i=1;
       while (i<=n){
           System.out.print("Enter an  number:  ");//input nilam
           int a = sc.nextInt();
           if (a>0){
           poscounter++;
           i++;
           }
           else if (a<0){
               negcounter++;
               i++;
          
       }
       } 
        System.out.println(poscounter+" NON NEGATIVE NUMBERS");//output
         System.out.println(negcounter+" Negative Numbers”);//arekta output
       
    
}
}


//6
import java.util.Scanner;
public class test {
 public static void main(String[]args){
 Scanner sc= new Scanner(System.in);
 System.out.print("Enter number of terms= ");//prompt
int a=sc.nextInt();//input

int i= 1;
int sum= 0;
int a1=1;

while (i<=a) {//wow loop very cool

System.out.println (a1);//print 
sum= sum+a;
i++;
a1=a1+2;
}
System.out.printf("The Sum of odd Natural Numbers up to %d terms is: %d",a,sum);//output wow
 }

}


