import java.util.Scanner ;
public class test {
    public static void main(String[]args){
        Scanner sc = new Scanner(System.in);
        int i=1;
        int sum=0;
       while (i<=10){
           System.out.print("Enter number:  ");//prompt dilam 
           int a = sc.nextInt();//input nilam
           sum= sum+a;
           System.out.println("Sum = "+sum);//sum ber hoche
           i++;// post increment hoche
           
       
       }
    
}
}
