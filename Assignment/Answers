import java.util.Scanner;
public class test {
 public static void main (String[]args){
 Scanner sc= new Scanner(System.in);
 System.out.print("enter your first number= ");
int num1=sc.nextInt();
System.out.print("enter your second number= ");
int num2=sc.nextInt();
System.out.print("enter your third number= " );
int num3=sc.nextInt();
 if (num1>num2 && num1>num3) {
 System.out.println("largest number is "+num1);
 }
else if (num2>num1 && num2>num3) {
 System.out.println("largest number is "+num2);
 }
else {
System.out.println("largest number is "+num3);
 }
}
}


//2
import java.util.Scanner;
public class test {
public static void main (String[]args){
 Scanner sc= new Scanner(System.in);
 System.out.print("enter your number= ");
 int num1=sc.nextInt();
if (num1>=90 && num1<=100) { 
 System.out.println("A Grade");
 }
 else if (num1>=85 && num1<=89) { 
 System.out.println("A- Grade");
 }
else if (num1>=70 && num1<=84) {
System.out.println("B Grade");
 }
else if (num1>=57 && num1<=69) {
System.out.println("C Grade");
 }
else if (num1>=50 && num1<=56){
System.out.println("D Grade");
 }
else {
System.out.print("Fail");
}
}
}









//3
import java.util.Scanner;
public class test1223 {
 public static void main (String[]args){
 Scanner sc= new Scanner(System.in);
 System.out.print("enter your first number= ");
 int num1=sc.nextInt();
System.out.print("enter your second number= ");
 int num2=sc.nextInt();
 sc.nextLine();
System.out.print("enter your operator= ");
 String act=sc.nextLine();
 if (act.equals("+")) {
 int sum= num1+num2;
System.out.println("sum is "+sum);
 }
if (act.equals("-")) {
int sub= num1-num2;
 System.out.println("subtitution is "+sub);
 }
if (act.equals("*")) {
 int mul= num1*num2;
 System.out.println("multiplication is "+mul);
 }
if (act.equals("/")) {
double div= ((num1*1.0)/num2);
 System.out.printf("division is "+div);
 }
}
}


//4

import java.util.Scanner;
public class test1223 {
public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);
        System.out.print("Enter your number= ");
        int number = scanner.nextInt();
           if (number % 5 == 0 && number % 7 == 0) {
            System.out.println("Divisible by Both");
        } else if (number % 5 == 0) {
            System.out.println("Invalid: Divisible by 5 Only");
        } else if (number % 7 == 0) {
            System.out.println("Invalid: Divisible by 7 Only");
        } else {
            System.out.println("No");
        }
}
}

